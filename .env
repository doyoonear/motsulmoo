# Google Cloud API Keys
GOOGLE_CLOUD_API_KEY=your_google_cloud_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_API_SECRET=your_supabase_secret_key_here

# Database URLs (Supabase PostgreSQL - Session Pooler)
# 주의: 비밀번호에 특수문자가 있으면 URL 인코딩 필요 (예: ! → %21)
DATABASE_URL=postgresql://postgres.your-project:your-password@aws-region.pooler.supabase.com:5432/postgres
